{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": "off",
        "react/prefer-stateless-function": "off",
        "react/prop-types": "off",
        
        "max-len": 0,
        "import/extensions": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "no-shadow": 0
    }
}